{
  "name": "example",
  "version": "7.0.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 16 Pro\"",
    "start": "react-native start",
    "pods:nuke": "cd ios && rm -rf Pods && rm -rf build && rm -f Podfile.lock && USE_FRAMEWORKS=static bundle exec pod install --repo-update && rm -f ios/.xcode.env.local",
    "pods": "cd ios && USE_FRAMEWORKS=static bundle exec pod install --repo-update && rm -f ios/.xcode.env.local",
    "android:logcat": "adb logcat",
    "nuke": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && rm -rf ./ios/Pods && yarn",
    "codegen": "./codegen.sh",
    "build:android": "cd android && bundle exec fastlane android build_example",
    "build:ios": "cd ios && bundle exec fastlane build_example",
    "release:android": "cd android && bundle exec fastlane distribute",
    "release:ios": "cd ios && bundle exec fastlane distribute",
    "e2e:build-ios": "detox build --configuration ios.sim.debug",
    "e2e:run-ios": "detox test --configuration ios.sim.debug",
    "e2e:build-android": "detox build --configuration android.emu.debug",
    "e2e:run-android": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-clipboard/clipboard": "^1.14.3",
    "@react-native-firebase/analytics": "^21.2.0",
    "@react-native-firebase/app": "^21.2.0",
    "@react-native-firebase/dynamic-links": "^21.2.0",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "2.28.0",
    "react-native-permissions": "^5.0.2",
    "react-native-reanimated": "4.1.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-worklets": "^0.5.1",
    "reanimated-color-picker": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.26.10",
    "@react-native-community/cli": "20.0.2",
    "@react-native-community/cli-platform-android": "20.0.2",
    "@react-native-community/cli-platform-ios": "20.0.2",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "0.81.1",
    "@react-native/metro-config": "0.81.1",
    "@react-native/typescript-config": "0.81.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "detox": "^20.38.0",
    "jest": "^29",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=20"
  },
  "workspaces": {
    "nohoist": [
      "react-native-reanimated",
      "react-native-reanimated/**",
      "react-native-worklets",
      "react-native-worklets/**"
    ]
  }
}
