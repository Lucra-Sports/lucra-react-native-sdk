{
  "name": "example",
  "version": "7.0.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 16 Pro\"",
    "start": "react-native start",
    "pods": "cd ios && rm -rf Pods && rm -f Podfile.lock && USE_FRAMEWORKS=static bundle exec pod install --repo-update && rm -f ios/.xcode.env.local",
    "android:logcat": "adb logcat",
    "nuke": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && rm -rf ./ios/Pods && yarn",
    "codegen": "./codegen.sh",
    "build:android": "cd android && bundle exec fastlane android build_example",
    "build:ios": "cd ios && bundle exec fastlane build_example",
    "release:android": "cd android && bundle exec fastlane distribute",
    "release:ios": "cd ios && bundle exec fastlane distribute",
    "e2e:build-ios": "detox build --configuration ios.sim.debug",
    "e2e:run-ios": "detox test --configuration ios.sim.debug",
    "e2e:build-android": "detox build --configuration android.emu.debug",
    "e2e:run-android": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-clipboard/clipboard": "^1.14.3",
    "@react-native-firebase/analytics": "^21.2.0",
    "@react-native-firebase/app": "^21.2.0",
    "@react-native-firebase/dynamic-links": "^21.2.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "nativewind": "^4.1.21",
    "react": "18.3.1",
    "react-native": "^0.76.9",
    "react-native-date-picker": "^5.0.12",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-permissions": "^5.0.2",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.35.0",
    "react-native-vector-icons": "^10.2.0",
    "reanimated-color-picker": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-android": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-ios": "15.0.0-alpha.2",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "@types/react": "^18.3.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "detox": "^20.35.0",
    "jest": "^29",
    "tailwindcss": "^3.4.14"
  },
  "engines": {
    "node": ">=20"
  }
}
