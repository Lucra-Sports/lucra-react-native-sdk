{
  "name": "LucrasdkExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "pods": "pod-install --quiet",
    "android:logcat": "adb logcat",
    "nuke": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && rm -rf ./ios/Pods && yarn && npx pod-install",
    "postinstall": "patch-package",
    "codegen": "./codegen.sh"
  },
  "dependencies": {
    "@lucra-sports/lucra-react-native-sdk": "../.",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.9.13",
    "nativewind": "^2.0.11",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-native": "^0.73.0",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.21.0",
    "react-native-vector-icons": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "^0.73.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
